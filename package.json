{
  "name": "bunni-development",
  "license": "UNLICENSED",
  "scripts": {
    "build:mainnet": "graph build subgraph.mainnet.yaml",
    "build:goerli": "graph build subgraph.goerli.yaml",
    "prepare:mainnet": "mustache ./config/mainnet.json manifest.template.yaml > subgraph.mainnet.yaml && graph codegen subgraph.mainnet.yaml --output-dir src/types/",
    "prepare:goerli": "mustache ./config/goerli.json manifest.template.yaml > subgraph.goerli.yaml && graph codegen subgraph.goerli.yaml --output-dir src/types/",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy bunniapp/bunni-mainnet-development subgraph.yaml --node https://api.thegraph.com/deploy/",
    "create-local": "graph create --node http://localhost:8020/ bunniapp/bunni-mainnet-development",
    "remove-local": "graph remove --node http://localhost:8020/ bunniapp/bunni-mainnet-development",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 bunniapp/bunni-mainnet-development"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.46.1",
    "@graphprotocol/graph-ts": "^0.29.3",
    "mustache": "^4.2.0",
    "source-map-support": "^0.5.21"
  }
}
